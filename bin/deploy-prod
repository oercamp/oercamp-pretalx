#!/bin/bash
sudo -u pretalxuser -H git pull
docker compose exec -u pretalxuser -T pretalx python3 /pretalx/src/manage.py makemigrations
docker compose exec -u pretalxuser -T pretalx python3 /pretalx/src/manage.py migrate
docker compose exec -u pretalxuser -T pretalx python3 /pretalx/src/manage.py rebuild
